SELECT * from sales;

--Add a new column for Sales that will allow you to see whose package requires a UPS shipping label and whose requires a DHL International label.
SELECT name, country, product,  
    CASE
        WHEN country IN("United states") THEN "UPS"
        ELSE "DHL International"
    END as "shipping_label"
FROM sales;

--Find the amount of labels needed for each type of shipping service.
SELECT COUNT(*),
    CASE
        WHEN country IN("United states") THEN "UPS"
        ELSE "DHL International"
    END as "shipping_label"
FROM sales
GROUP by shipping_label;

--Find the amount of earnings made for each type of product. 
SELECT product, SUM(price)
AS JAN_TOTAL_EARNINGS
FROM sales
GROUP BY product;
